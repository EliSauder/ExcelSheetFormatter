<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationConfiguration">
      <section name="directories" type="Formatter.Configuration.ConfigurationSectionFiles,BomFormatter" />
      <section name="boms" type="Formatter.Configuration.ConfigurationSectionBoms,BomFormatter" />
    </sectionGroup>
  </configSections>
  
  <startup> 
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>

  <applicationConfiguration>
    
    <directories rootDirectory="C:\Users\esauder\OneDrive - HME\Documents\TestFormatting\" inputFolder="input\" outputFolder="output\" logFolder="" oldInputFolder="" />

    <boms>
      <bom name="axToPT" outputType="INDIVIDUAL" outputSheetName="IMPORTBOM" 
           inputFileExtention=".xlsx" displayName="AX -&gt; Process Tracker" enabled="True">
        <fields>
          <field name="partNumber" header="Item number" output="PartNumber" required="True" order="0" 
                 dataType="System.String" identifierOrder="0" enabled="True">
            <cleanupActions>
              <cleanup active="True" name="toUpper" action="UPPERCASE" condition="ANY" scope="CELL" 
                       report="False"/>
            </cleanupActions>
          </field>
          <field name="partDescription" header="Product name" output="PartDescription" required="True" 
                 order="1" dataType="System.String" enabled="True" identifierOrder="0" />
          <field name="partTemplate" header="" output="PartTemplate" required="False" order="2" 
                 dataType="System.String" enabled="True" />
          <field name="quantity" header="Quantity" output="QTY" required="True" order="3" 
                 dataType="System.Double" isQuantity="True" enabled="True">
            <cleanupActions>
              <cleanup name="zeroQantity" action="REMOVE" condition="EQUALS" value="0" scope="ROW" 
                       report="True"/>
              <cleanup name="decimalQuantity" action="REPORT" condition="ISFLOAT" scope="CELL" 
                       report="True"/>
            </cleanupActions>
          </field>
          <field name="referenceDesignator" header="Reference designator" output="RD" required="True" 
                 order="4" dataType="System.String" isSplit="True" delimiter=" " override="True" enabled="True" identifierOrder="2">
            <populations>
              <population name="pcb" condition="EQUALS" findValue="1" setValue="pcb" toColumn="partTemplate" 
                          active="True" />
            </populations>
          </field>
          <field name="position" header="Position" output="" required="True" order="4" 
                 dataType="System.String" enabled="True" />
          <field name="alternatePartNumber" header="" output="AlternatePartNumber" required="False" 
                 order="5" dataType="System.String" enabled="True" />
          <field name="subProductReferenceNumber" header="" output="SubProductReferenceNumber" 
                 required="False" order="6" dataType="System.String" enabled="True" />
        </fields>  
      </bom>

      <bom name="agileToPT" outputType="INDIVIDUAL" outputSheetName="IMPORTBOM" inputFileExtention=".xls" 
           displayName="Agile -&gt; Process Tracker" enabled="True">
        <fields>
          <field name="partNumber" header="Item Number" output="PartNumber" required="True" order="0" 
                 dataType="System.String" identifierOrder="0" enabled="True">
            <cleanupActions>
              <cleanup active="True" name="toUpper" action="UPPERCASE" condition="ANY" scope="CELL" 
                       report="False"/>
            </cleanupActions>
          </field>
          <field name="partDescription" header="Description" output="PartDescription" required="True" 
                 order="1" dataType="System.String" enabled="True" identifierOrder="0" />
          <field name="partTemplate" header="BOM.Mounting Style" output="PartTemplate" required="False" 
                 order="2" dataType="System.String" enabled="True">
            <populations>
              <population name="smdToSmt" condition="EQUALS" findValue="SMD" setValue="smt" 
                          toColumn="partTemplate" active="True"/>
            </populations>
          </field>
          <field name="quantity" header="BOM.Qty" output="QTY" required="True" order="3" 
                 dataType="System.Double" isQuantity="True" enabled="True">
            <cleanupActions>
              <cleanup name="zeroQantity" action="REMOVE" condition="EQUALS" value="0" scope="ROW" 
                       report="True"/>
              <cleanup name="decimalQuantity" action="REPORT" condition="ISFLOAT" scope="CELL" 
                       report="True"/>
            </cleanupActions>
          </field>
          <field name="referenceDesignator" header="BOM.Ref Des" output="RD" required="True" order="4" 
                 dataType="System.String" isSplit="True" delimiter="," override="True" enabled="True" 
                 identifierOrder="2">
            <populations>
              <population name="pcb" condition="EQUALS" findValue="1" setValue="pcb" toColumn="partTemplate" 
                          active="True" />
            </populations>
          </field>
          <field name="alternatePartNumber" header="" output="AlternatePartNumber" required="False" 
                 order="5" dataType="System.String" enabled="True" />
          <field name="subProductReferenceNumber" header="" output="SubProductReferenceNumber" 
                 required="False" order="6" dataType="System.String" enabled="True" />
        </fields>
      </bom>
    </boms>

  </applicationConfiguration>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="DocumentFormat.OpenXml" publicKeyToken="8fb06cb64d019a17" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.10.1.0" newVersion="2.10.1.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>